      <lp name="open-sesame" version="commit-96639aedd24442433365d4d9fca877931fe222e4" timestamp="2021-02-12T06:15:51CET" beginTimestamp="2021-02-12T04:00:52UTC" endTimestamp="2021-02-12T05:14:40UTC" hostname="kyoto"/>
