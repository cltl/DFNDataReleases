      <lp name="open-sesame" version="commit-96639aedd24442433365d4d9fca877931fe222e4" timestamp="2021-02-11T22:21:11CET" beginTimestamp="2021-02-11T20:03:36UTC" endTimestamp="2021-02-11T21:19:53UTC" hostname="kyoto"/>
