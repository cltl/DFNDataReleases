      <lp name="open-sesame" version="commit-96639aedd24442433365d4d9fca877931fe222e4" timestamp="2021-02-11T21:02:24CET" beginTimestamp="2021-02-11T18:46:19UTC" endTimestamp="2021-02-11T20:01:20UTC" hostname="kyoto"/>
